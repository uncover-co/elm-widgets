{
  "name": "elm-widgets-alpha",
  "version": "11.0.1",
  "description": "A collection of stateless, themable widgets for Elm applications.",
  "scripts": {
    "postinstall": "elm-tooling install",
    "test": "elm-test-rs",
    "start": "concurrently \"npm:watch:*\"",
    "watch:styles": "watch 'node ./styles/build-styles.js' ./styles/output ./src/Template/W",
    "watch:tailwind": "tailwindcss -i ./styles/index.css -o ./styles/output/index.css --minify --watch",
    "docs": "elm-doc-preview"
  },
  "author": "Georges Boris",
  "license": "MPL-2.0",
  "devDependencies": {
    "autoprefixer": "^10.4.1",
    "concurrently": "^7.1.0",
    "cssnano": "^5.0.14",
    "elm-doc-preview": "^5.0.5",
    "elm-review": "^2.8.5",
    "elm-theme-tailwindcss": "^2.0.1",
    "elm-tooling": "^1.7.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.1.8",
    "watch": "^1.0.2"
  }
}
